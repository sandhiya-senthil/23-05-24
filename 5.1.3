n = int(input("Enter the number of elements: "))
numbers = []
for i in range(n):
    num = float(input("Enter number {}: ".format(i+1)))
    numbers.append(num)
largest = numbers[0]
smallest = numbers[0]
for num in numbers:
    if num > largest:
        largest = num
    elif num < smallest:
        smallest = num
print("Largest number:", largest)
print("Smallest number:", smallest)
