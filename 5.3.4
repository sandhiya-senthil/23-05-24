array1 = list(map(int, input("Enter the elements of the first array separated by space: ").split()))
array2 = list(map(int, input("Enter the elements of the second array separated by space: ").split()))
max_element_array1 = max(array1)
min_element_array2 = min(array2)
product = max_element_array1 * min_element_array2
print("Product of max element of first array and min element of second array:", product)
